{
  "name": "@antd-templater/library-3.x",
  "description": "基于 Vue3.x Ant Design Vue 对Tree、Table、Form等组件二次封装",
  "homepage": "https://github.com/antd-templater/library-3.x",
  "author": "lin pengteng <china.linpengteng@gmail.com>",
  "license": "MIT",
  "version": "4.1.1",
  "keywords": [
    "form",
    "tree",
    "table",
    "front-end",
    "ant-design-vue",
    "vue-components"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "import": "./dist/resolver.mjs",
      "require": "./dist/resolver.cjs"
    },
    "./globals": {
      "types": "./dist/globals.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antd-templater/library-3.x.git"
  },
  "bugs": {
    "url": "https://github.com/antd-templater/library-3.x/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "shx rm -rf dist && rollup --config rollup.config.ts --configPlugin typescript",
    "eslint": "npx eslint --fix --quiet src --ext .vue,.js,ts,jsx,tsx",
    "release": "release-it",
    "prepare": "husky"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.1",
    "dayjs": "^1.11.11",
    "js-simpler": "^1.5.0",
    "less": "^4.2.0",
    "vue": "^3.4.26",
    "vue-router": "^4.3.2",
    "vue-types": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@ctrl/tinycolor": "^4.1.0",
    "@release-it/conventional-changelog": "^8.0.1",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@typescript-eslint/typescript-estree": "^7.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.4.26",
    "cz-message-helper": "^1.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.25.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "release-it": "^17.2.1",
    "rollup": "^4.17.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-vue": "^6.0.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "volar-service-prettyhtml": "^0.0.42",
    "vue-template-compiler": "^2.7.16",
    "vue-tsc": "^2.0.16"
  },
  "config": {
    "cz-message-helper": {
      "config": ".cz-message.cjs"
    },
    "commitizen": {
      "path": "node_modules/cz-message-helper"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}